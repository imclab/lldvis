	<%= stylesheet_link_tag "jquery-ui-1.8.4.custom" %>
	<%= stylesheet_link_tag "ui.jqgrid" %>
	<%= stylesheet_link_tag "main" %>

	<%= javascript_include_tag "jquery-1.4.2.min" %>
	<%= javascript_include_tag "jquery-ui-1.8.4.custom.min" %>
	<%= javascript_include_tag "jquery.tablesorter.min" %>
	<%= javascript_include_tag "jquery.metadata" %>
	<%= javascript_include_tag "jquery.jqGrid.min" %>
	<%= javascript_include_tag "application" %>
	
	<script>
		$(document).ready(function() {
			$.tablesorter.defaults.widgets = ['zebra'];
			$.tablesorter.defaults.textExtraction = 'complex'
			$("table.panel-grid").tablesorter();
		} 
		);
	</script>
	
	<%= yield :head %>
</head>
