<h1>Editing usecasecluster</h1>

<% form_for(@usecasecluster) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @usecasecluster %> |
<%= link_to 'Back', usecaseclusters_path %>